{
"manifest_version": 3,
"name": "pyMedia 下载接管插件",
"version": "1.0",
"description": "接管浏览器下载并发送至 pyMedia 下载管理器",
"permissions": [
"downloads",
"downloads.shelf",
"storage",
"notifications",
"cookies"
],
"host_permissions": [
"http://localhost/*",
"<all_urls>"
],
"action": {
"default_popup": "popup.html"
},
"background": {
"service_worker": "background.js"
}
}